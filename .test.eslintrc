{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true
  },
  "globals" : {
    "describe": false,
    "it": false,
    "expect": false
  },
  "rules": {
    "import/no-unresolved": "off",
    "no-underscore-dangle": "off",
    "react/prefer-stateless-function": "off",
    "no-unused-expressions": "off"
  }
}